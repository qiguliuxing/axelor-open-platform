<?xml version="1.0"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models https://axelor.com/xml/ns/domain-models/domain-models_8.0.xsd">

  <module name="auth" package="com.axelor.auth.db" />

  <entity name="UserToken" cacheable="true" repository="abstract">
    <string name="name" title="Name" required="true" index="false"/>
    <datetime name="expiresAt" title="Expires" required="true" />
    <string name="tokenKey" unique="true" required="true" password="true" readonly="true" />
    <string name="tokenDigest" title="Token" required="true" password="true" readonly="true" />
    <string name="apiKey" title="API key" transient="true" password="true" readonly="true" />
    <datetime name="lastUsedAt" title="Last used" readonly="true" />
    <many-to-one name="owner" title="User" ref="com.axelor.auth.db.User" required="true" readonly="true" />
  </entity>

</domain-models>
