<?xml version="1.0" ?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models https://axelor.com/xml/ns/domain-models/domain-models_8.0.xsd">

  <module name="test" package="com.axelor.test.db"/>

  <entity name="MoveLine" table="TEST_MOVE_LINE">

    <many-to-one name="move" ref="Move"/>
    <many-to-one name="invoiceReject" ref="Invoice"/>

    <date name="date" column="move_date"/>
    <date name="dueDate"/>

    <decimal name="credit" default="0"/>
    <decimal name="debit" default="0"/>

  </entity>

</domain-models>
